[ sql ] [ SQL ] INSERT INTO `ppsao_wenan` (`uid` , `title` , `content` , `intro` , `author` , `cover` , `catid` , `rhref` , `rhref_text` , `is_show_title` , `is_show_statis` , `is_vip` , `views` , `likes` , `is_tmp` , `updatetime` , `dateline`) VALUES (3 , '十九大期间电视播出的安全播出应急预案' , '&lt;p style=&quot;margin-top:10px;margin-right:1px;margin-bottom:0;margin-left:43px;margin-bottom:0;text-align:center;line-height:110%;vertical-align:baseline&quot;&gt;&lt;strong&gt;&lt;span style=&quot;font-size:21px;line-height: 110%;font-family:宋体&quot;&gt;&lt;br/&gt;&lt;/span&gt;&lt;/strong&gt;&lt;/p&gt;&lt;p style=&quot;margin-top:0;margin-right:1px;margin-bottom:0;margin-left:32px;margin-bottom:0;text-align:left;line-height:110%;vertical-align:baseline&quot;&gt;&lt;strong&gt;&lt;span style=&quot;font-size:16px;line-height:110%;font-family:宋体&quot;&gt;1&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style=&quot;font-size:16px;line-height:110%;font-family: 宋体&quot;&gt;、直播或转播期间，信号源出现非法内容或其它突发情况时应急处理办法：&lt;/span&gt;&lt;/strong&gt;&lt;/p&gt;&lt;p style=&quot;margin-top:0;margin-bottom:0;margin-left:28px;margin-bottom:0;text-align:left;line-height:110%;vertical-align:baseline&quot;&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;1.1&lt;/span&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;、直播或转播信号中出现非法声音、图像或字幕时，应立即切断该信号源，播出垫片。 &lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;margin-top:0;margin-right:1px;margin-bottom:0;margin-left:28px;margin-bottom:0;text-align:left;line-height:110%;vertical-align: baseline&quot;&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;1.2&lt;/span&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;、直播或转播时、若信号被干扰或出现黑场时，应选择未被干扰的备路信号切出，若主备路均受干扰或出现黑场，立即切垫片。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;margin-top:0;margin-right:1px;margin-bottom:0;margin-left:28px;margin-bottom:0;text-align:left;line-height:110%;vertical-align: baseline&quot;&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;1.3&lt;/span&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;、直播或转播时信号受气象等自然因素影响，画面出现严重马赛克、黑场等情况，技术人员应立即采取必要措施，设法用较好的信号播出。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;margin-top:0;margin-right:1px;margin-bottom:0;margin-left:28px;margin-bottom:0;text-align:left;line-height:110%;vertical-align: baseline&quot;&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;1.4&lt;/span&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;、应急处理后，立即向本部门主任汇报。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;margin-top:0;margin-right:1px;margin-bottom:0;margin-left:28px;margin-bottom:0;text-align:left;line-height:110%;vertical-align: baseline&quot;&gt;&lt;strong&gt;&lt;span style=&quot;font-family:宋体;color:blue&quot;&gt;1.5&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style=&quot;font-family:宋体;color:blue&quot;&gt;、十九大期间，重点关注包括针对十九大的非法声音、图像或字幕的内容。&lt;/span&gt;&lt;/strong&gt;&lt;/p&gt;&lt;p style=&quot;margin-top:0;margin-right:1px;margin-bottom:0;margin-left:32px;margin-bottom:0;text-align:left;line-height:110%;vertical-align: baseline&quot;&gt;&lt;strong&gt;&lt;span style=&quot;font-size:16px;line-height:110%;font-family: 宋体&quot;&gt;&amp;nbsp;&lt;/span&gt;&lt;/strong&gt;&lt;/p&gt;&lt;p style=&quot;margin-top:0;margin-right:1px;margin-bottom:0;margin-left:32px;margin-bottom:0;text-align:left;line-height:110%;vertical-align: baseline&quot;&gt;&lt;strong&gt;&lt;span style=&quot;font-size:16px;line-height:110%;font-family: 宋体&quot;&gt;2&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style=&quot;font-size:16px;line-height:110%;font-family: 宋体&quot;&gt;、标清系统播出节目或节目单不能及时到达的应急处理办法&lt;/span&gt;&lt;/strong&gt;&lt;/p&gt;&lt;p style=&quot;margin-top:0;margin-right:1px;margin-bottom:0;margin-left:28px;margin-bottom:0;text-align:left;line-height:110%;vertical-align:baseline&quot;&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;2.1&lt;/span&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;、当节目单因网络原因无法按时传到播出机房时，应立即通知总编室相关人员用机房专用U盘直接拷贝，然后在播出机房的编单机上直接编单。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;margin-top:0;margin-right:1px;margin-bottom:0;margin-left:28px;margin-bottom:0;text-align:left;line-height:110%;vertical-align:baseline&quot;&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;2.2&lt;/span&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;、当磁带较晚送达上载机房，已形成播出隐患时，采用应急上载的方式上载节目素材。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;margin-top:0;margin-right:1px;margin-bottom:0;margin-left:28px;margin-bottom:0;text-align:left;line-height:110%;vertical-align:baseline&quot;&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;2.3&lt;/span&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;、当磁带较晚送达上载机房，已形成播出事故时，在播出时间前5分钟做好应急录像机播出的准备工作，并与总编室联系节目单的修改措施。送带人应进入播出机房，确定节目片头画面，待播出结束之后方可离开。播出员应严格按照修改后的节目单执行。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;margin-top:0;margin-right:1px;margin-bottom:0;margin-left:28px;margin-bottom:0;text-align:left;line-height:110%;vertical-align:baseline&quot;&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; &lt;/span&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;在播出时间前5-10分钟送达上载机房的磁带的处理详见《播出上载约定》。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;margin-top:0;margin-right:1px;margin-bottom:0;margin-left:28px;margin-bottom:0;text-align:left;line-height:110%;vertical-align:baseline&quot;&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;2.4&lt;/span&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;、当广告文件因光缆中断无法按时传送到广告缓存区时，广告中心将以移动硬盘将素材送到上载机房，上载人员立即采用本地广告传输的应急模式。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;margin-top:0;margin-right:1px;margin-bottom:0;margin-left:28px;margin-bottom:0;text-align:left;line-height:110%;vertical-align:baseline&quot;&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;2.5&lt;/span&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;、当广告文件因网关故障无法按时传送到广告缓存区时，广告中心将以磁带的形式将素材送到上载机房，上载人员立即采用磁带上载的应急模式。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;margin-top:0;margin-right:1px;margin-bottom:0;margin-left:28px;margin-bottom:0;text-align:left;line-height:110%;vertical-align:baseline&quot;&gt;&lt;span style=&quot;font-family:宋体;color:blue&quot;&gt;2.6&lt;/span&gt;&lt;span style=&quot;font-family:宋体;color:blue&quot;&gt;、十九大期间播出及上载人员应重点关注有关十九大的报道节目，在应急处理的时候应重点保障此类节目的应急播出。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;margin-top:0;margin-right:1px;margin-bottom:0;margin-left:28px;margin-bottom:0;text-align:left;line-height:110%;vertical-align:baseline&quot;&gt;&lt;span style=&quot;font-family:宋体;color:blue&quot;&gt;2.7&lt;/span&gt;&lt;span style=&quot;font-family:宋体;color:blue&quot;&gt;、关于十九大的各类宣传片应给与重点统一保管，以备应急使用。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;margin-top:0;margin-right:1px;margin-bottom:0;margin-left:32px;margin-bottom:0;text-align:left;line-height:110%;vertical-align:baseline&quot;&gt;&lt;span style=&quot;font-size:16px;line-height:110%;font-family:宋体&quot;&gt;&amp;nbsp;&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;margin-top:0;margin-right:1px;margin-bottom:0;margin-left:32px;margin-bottom:0;text-align:left;line-height:110%;vertical-align: baseline&quot;&gt;&lt;strong&gt;&lt;span style=&quot;font-size:16px;line-height:110%;font-family:宋体&quot;&gt;3、&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style=&quot;font-size:16px;line-height: 110%;font-family:宋体&quot;&gt;标清系统播出系统出现技术性问题的应急处理办法&lt;/span&gt;&lt;/strong&gt;&lt;/p&gt;&lt;p style=&quot;margin-left:28px;text-align:left;line-height:110%;vertical-align: baseline&quot;&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;3.1&lt;/span&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;、若监视墙上PGM和控制台上的PGM中只有一个有信号，另一个无信号时，立即通知技术人员，更换相应的输出分配板。&lt;strong&gt;&lt;/strong&gt;&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;margin-left:28px;text-align:left;line-height:110%;vertical-align: baseline&quot;&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;3.2&lt;/span&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;、若监视墙和控制台上的PGM都无信号，BYPASS有信号时，说明主播出板故障，立即切BYPASS键，由备播出板输出信号。当主备播出板都有故障时,马上通过跳线从测试信号(黄色跳线口)把相应信号源跳出。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;margin-left:28px;text-align:left;line-height:110%;vertical-align: baseline&quot;&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;3.3&lt;/span&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;、若监视墙和控制台上的PGM都无信号，BYPASS也无信号时，立即手动切换垫片信号，然后查看信号源监视器的情况，分别进行以下操作:&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;margin-top:0;margin-right:1px;margin-bottom:0;margin-left:28px;margin-bottom:0;text-align:left;line-height:110%;vertical-align:baseline&quot;&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;&amp;nbsp; 3.3.1&lt;/span&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;：当视频服务器信号播出时，主通道无信号，备通道有信号，立即手动切到备通道，准备磁带跟播。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;margin-top:0;margin-right:1px;margin-bottom:0;margin-left:28px;margin-bottom:0;text-align:left;line-height:110%;vertical-align:baseline&quot;&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;&amp;nbsp; 3.3.2&lt;/span&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;：现场直播时主信号故障，备信号正常时，立即手动切到备信号。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;margin-top:0;margin-right:1px;margin-bottom:0;margin-left:28px;margin-bottom:0;text-align:left;line-height:110%;vertical-align:baseline&quot;&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;3.4&lt;/span&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;、当上载存储器无法向播出存储器传送素材时，应采用应急上载模式。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;margin-top:0;margin-right:1px;margin-bottom:0;margin-left:28px;margin-bottom:0;text-align:left;line-height:110%;vertical-align:baseline&quot;&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;3.5&lt;/span&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;、当主备视频服务器同时出现故障，或者主备数据库同时出现故障时，用应急录像机播出，播出员有权变更节目单，播出长节目。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;margin-left:28px;text-align:left;line-height:110%;vertical-align: baseline&quot;&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;3.6&lt;/span&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;、当主播控机因故无法正常工作，又不能自动倒换到备播控机时，应手动倒换到备播控机。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;margin-top:0;margin-right:1px;margin-bottom:0;margin-left:28px;margin-bottom:0;text-align:left;line-height:110%;vertical-align:baseline&quot;&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;3.7&lt;/span&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;当切换台上的BYPASS键切换失灵时，应在主切换器对应频道的播出板上手动拨BYPASS开关。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;margin-right:1px;text-align:left;line-height:110%;vertical-align:baseline&quot;&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;&amp;nbsp;&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;margin-right:1px;text-align:left;line-height:110%;vertical-align:baseline&quot;&gt;&lt;strong&gt;&lt;span style=&quot;font-size:16px;line-height:110%;font-family:宋体&quot;&gt;4&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style=&quot;font-size:16px;line-height:110%;font-family: 宋体&quot;&gt;、高清播出系统出现技术性问题的应急处理办法&lt;/span&gt;&lt;/strong&gt;&lt;/p&gt;&lt;p style=&quot;margin-top:0;margin-right:1px;margin-bottom:0;margin-left:28px;margin-bottom:0;text-align:left;line-height:110%;vertical-align:baseline&quot;&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;4.1&lt;/span&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;、若监视墙上1-HD-M、1-HD-B信号异常时，需要查看监视墙上三选一输入信号监视点，分别进行以下操作:&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;margin-top:0;margin-right:1px;margin-bottom:0;margin-left:28px;margin-bottom:0;text-align:left;line-height:110%;vertical-align:baseline&quot;&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;4.1.1&lt;/span&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;、若1-HD-M和1-HD-B一个有信号一个无信号时，立即通知技术人员，联系下游单位。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;margin-top:0;margin-right:1px;margin-bottom:0;margin-left:28px;margin-bottom:0;text-align:left;line-height:110%;vertical-align:baseline&quot;&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;4.1.2&lt;/span&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;、若1-HD-M和1-HD-B都无信号，观察到HD-1无信号，HD-2信号正常，立即手动切至HD-2。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;margin-top:0;margin-right:1px;margin-bottom:0;margin-left:28px;margin-bottom:0;text-align:left;line-height:110%;vertical-align:baseline&quot;&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;4.1.3&lt;/span&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;、若1-HD-M和1-HD-B都无信号，观察到HD-1和HD-2无信号、HD-3信号正常，立即手动切至HD-3。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;margin-top:0;margin-right:1px;margin-bottom:0;margin-left:28px;margin-bottom:0;text-align:left;line-height:110%;vertical-align:baseline&quot;&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;4.1.4&lt;/span&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;、若1-HD-M和1-HD-B都无信号，观察到HD-1、HD-2、HD-3均无信号，检查信号源，切换正常信号。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;margin-top:0;margin-right:1px;margin-bottom:0;margin-left:28px;margin-bottom:0;text-align:left;line-height:110%;vertical-align:baseline&quot;&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;4.2&lt;/span&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;、若监视墙上1-SD-M、1-SD-B信号异常时，需要查看监视墙上三选一输入信号监视点，分别进行以下操作:&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;margin-top:0;margin-right:1px;margin-bottom:0;margin-left:28px;margin-bottom:0;text-align:left;line-height:110%;vertical-align:baseline&quot;&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;4.1.1&lt;/span&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;、若1-SD-M和1-SD-B一个有信号一个无信号时，立即通知技术人员，联系下游单位。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;margin-top:0;margin-right:1px;margin-bottom:0;margin-left:28px;margin-bottom:0;text-align:left;line-height:110%;vertical-align:baseline&quot;&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;4.1.2&lt;/span&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;、若1-SD-M和1-SD-B都无信号，观察到SD-1无信号，SD-2信号正常，立即手动切至SD-2。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;margin-top:0;margin-right:1px;margin-bottom:0;margin-left:28px;margin-bottom:0;text-align:left;line-height:110%;vertical-align:baseline&quot;&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;4.1.3&lt;/span&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;、若1-SD-M和1-SD-B都无信号，观察到SD-1和SD-2无信号、HD-3信号正常，立即手动切至SD-3。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;margin-top:0;margin-right:1px;margin-bottom:0;margin-left:28px;margin-bottom:0;text-align:left;line-height:110%;vertical-align:baseline&quot;&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; 4.1.4&lt;/span&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;、若1-SD-M和1-SD-B都无信号，观察到SD-1、SD-2、SD-3均无信号，检查信号源，切换正常信号。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;margin-top:0;margin-right:1px;margin-bottom:0;margin-left:28px;margin-bottom:0;text-align:left;line-height:110%;vertical-align:baseline&quot;&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;4.3&lt;/span&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;、当主、备、二备视频服务器同时出现故障，或者主备数据库同时出现故障时，用应急录像机播出，播出员有权变更节目单，播出长节目。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;margin-top:0;margin-right:1px;margin-bottom:0;margin-left:28px;margin-bottom:0;text-align:left;line-height:110%;vertical-align:baseline&quot;&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;4.4&lt;/span&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;、当上载服务器无法向播出服务器传送素材时，应采用应急上载模式。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;margin-left:28px;text-align:left;line-height:110%;vertical-align: baseline&quot;&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;4.5&lt;/span&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;、当主播控机因故无法正常工作，又不能自动倒换到备播控机时，应手动倒换到备播控机。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;margin-left:28px;text-align:left;line-height:110%;vertical-align: baseline&quot;&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;&amp;nbsp;&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;strong&gt;&lt;span style=&quot;font-size:16px;font-family: 宋体&quot;&gt;5&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style=&quot;font-size:16px;font-family:宋体&quot;&gt;．供配电故障的应急处理办法&lt;/span&gt;&lt;/strong&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;;font-family:宋体&quot;&gt;5.1&lt;/span&gt;&lt;span style=&quot;;font-family:宋体&quot;&gt;、当配电柜中有空开等故障引起多只插座没电时，应立即将相关的设备电源线接到正常的插座上，或者通过应急接线板转接。&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;;font-family:宋体&quot;&gt;5.2&lt;/span&gt;&lt;span style=&quot;;font-family:宋体&quot;&gt;、 当UPS供电发生故障时,应把单电源供电的设备(电源线上有特殊标志)接至市电一路。&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;;font-family:宋体&quot;&gt;&amp;nbsp;&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;strong&gt;&lt;span style=&quot;font-size:16px;font-family: 宋体;color:blue&quot;&gt;6&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style=&quot;font-size: 16px;font-family:宋体;color:blue&quot;&gt;、加强各项保障预案&lt;/span&gt;&lt;/strong&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;;font-family:宋体;color:blue&quot;&gt;6.1&lt;/span&gt;&lt;span style=&quot;;font-family:宋体;color:blue&quot;&gt;：加强人员保障：&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;;font-family:宋体;color:blue&quot;&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; &lt;/span&gt;&lt;span style=&quot;font-family:宋体;color:blue&quot;&gt;十九大期间&lt;/span&gt;&lt;span style=&quot;;font-family:宋体;color:blue&quot;&gt;确保24小时部门领导和全体技术人员的通讯畅通。加强晚间重要直播时段的值班力量，领导加强一线值班，随时应对应急事件和各类应急播出。加强待岗技术人员的数量，包括晚间10点后，如果有应急需要，随时延长。&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;;font-family:宋体;color:blue&quot;&gt;6.2&lt;/span&gt;&lt;span style=&quot;;font-family:宋体;color:blue&quot;&gt;、加强播出技术的保障：&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;;font-family:宋体;color:blue&quot;&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; &lt;/span&gt;&lt;span style=&quot;;font-family:宋体;color:blue&quot;&gt;随时准备配合总编室变更节目单，为有可能的直播或转播做准备。加强巡查总控调度和播控运行的情况，排除故障隐患。检查有可能使用的各路直播信号，确保光缆及卫星信号的正常。加强巡查GPS及北斗时钟天线及通道，确保正常。加强巡查播出机房到华数机房的光缆通道，确保正常。&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;;font-family:宋体;color:blue&quot;&gt;6.3&lt;/span&gt;&lt;span style=&quot;;font-family:宋体;color:blue&quot;&gt;、加强电力及空调保障的应对措施：当外电断电时，机房在UPS供电的情况下，积极配合行政安保部门对UPS用电的管理和调度。当楼层或机房空调出现故障时，采用各种补充手段保证机房温度正常。&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;;font-family:宋体;color:blue&quot;&gt;6.4&lt;/span&gt;&lt;span style=&quot;;font-family:宋体;color:blue&quot;&gt;、严格执行每日“零汇报”制度。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;margin-top:0;margin-right:1px;margin-bottom:0;margin-left:28px;margin-bottom:0;text-align:left;line-height:110%;vertical-align:baseline&quot;&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;&amp;nbsp;&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;margin-top:0;margin-right:1px;margin-bottom:0;margin-left:28px;margin-bottom:0;text-align:left;line-height:110%;vertical-align:baseline&quot;&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; &lt;/span&gt;&lt;strong&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;电视技术中心播出部&lt;/span&gt;&lt;/strong&gt;&lt;/p&gt;&lt;p style=&quot;margin-top:0;margin-right:1px;margin-bottom:0;margin-left:28px;margin-bottom:0;text-align:left;line-height:110%;vertical-align:baseline&quot;&gt;&lt;strong&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; 2017&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style=&quot;font-family:宋体&quot;&gt;年9月&lt;/span&gt;&lt;/strong&gt;&lt;/p&gt;&lt;p&gt;&lt;br/&gt;&lt;/p&gt;' , '' , '' , '' , 21 , '' , '阅读原文' , 1 , 1 , 1 , 0 , 0 , 0 , 1506651924 , 1506651840)                   [ RunTime:0.048500s ]
