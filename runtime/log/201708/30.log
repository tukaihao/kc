[ sql ] [ SQL ] UPDATE `ppsao_wenan`  SET `uid`=12,`title`='上载岗位操作细则',`content`='&lt;p style=&quot;text-align: center;&quot;&gt;&lt;strong&gt;&lt;span style=&quot;font-family: 宋体; font-size: 24px;&quot;&gt;上载岗位操作细则&lt;/span&gt;&lt;/strong&gt;&lt;/p&gt;&lt;p&gt;&lt;strong&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;标清系统：&lt;/span&gt;&lt;/strong&gt;&lt;/p&gt;&lt;p style=&quot;margin: 16px 0px 4px 28px;&quot;&gt;&lt;span style=&quot;letter-spacing: -3px; font-size: 19px;&quot;&gt;&lt;span style=&quot;font-family: Cambria;&quot;&gt;一&lt;/span&gt;&lt;span new=&quot;&quot; times=&quot;&quot;&gt;&amp;nbsp; &lt;/span&gt;&lt;/span&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;上载准备工作&lt;/span&gt;&lt;/p&gt;&lt;ol class=&quot; list-paddingleft-2&quot; style=&quot;list-style-type: decimal;&quot;&gt;&lt;li&gt;&lt;p style=&quot;color: rgb(0, 0, 0); font-size: 14px; font-style: normal; font-weight: normal; margin-top: 0px; margin-bottom: 0px;&quot;&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;按时到岗，认真工作。作为部门的窗口岗位，待人接物文明礼貌。&lt;/span&gt;&lt;/p&gt;&lt;/li&gt;&lt;li&gt;&lt;p style=&quot;color: rgb(0, 0, 0); font-family:;&quot; new=&quot;&quot; times=&quot;&quot;&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;做好交接班工作，仔细查看值班日记。&lt;/span&gt;&lt;/p&gt;&lt;/li&gt;&lt;/ol&gt;&lt;p style=&quot;margin: 16px 0px 4px 28px;&quot;&gt;&lt;span style=&quot;letter-spacing: -3px; font-size: 19px;&quot;&gt;&lt;span style=&quot;font-family: Cambria;&quot;&gt;二&lt;/span&gt;&lt;span new=&quot;&quot; times=&quot;&quot;&gt;&amp;nbsp; &lt;/span&gt;&lt;/span&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;磁带管理&lt;/span&gt;&lt;/p&gt;&lt;ol class=&quot; list-paddingleft-2&quot; style=&quot;list-style-type: decimal;&quot;&gt;&lt;li&gt;&lt;p style=&quot;color: rgb(0, 0, 0); font-size: 14px; font-style: normal; font-weight: normal; margin-top: 0px; margin-bottom: 0px;&quot;&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;周五下午需要接收周五至周一白天各频道栏目的磁