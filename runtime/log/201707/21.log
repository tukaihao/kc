[ sql ] [ SQL ] UPDATE `ppsao_wenan`  SET `uid`=12,`title`='播控岗位操作细则',`content`='&lt;p style=&quot;text-align: center;&quot;&gt;&lt;strong&gt;&lt;span style=&quot;font-family: 宋体; font-size: 21px;&quot;&gt;播控岗位操作细则&lt;/span&gt;&lt;/strong&gt;&lt;/p&gt;&lt;p style=&quot;text-align: left;&quot;&gt;&lt;strong&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;标清系统：&lt;/span&gt;&lt;/strong&gt;&lt;/p&gt;&lt;p style=&quot;text-align: left;&quot;&gt;&lt;strong&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;一、准备工作&lt;/span&gt;&lt;/strong&gt;&lt;/p&gt;&lt;p style=&quot;text-align: left; margin-left: 0px;&quot;&gt;1、&lt;span style=&quot;font: 9px/normal &amp;quot;Times New Roman&amp;quot;; font-size-adjust: none; font-stretch: normal;&quot;&gt;&amp;nbsp; &lt;/span&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;打开电视墙和播控台上面的所有监视器，并确认所有信号源正常。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align: left; margin-left: 0px;&quot;&gt;2、&lt;span style=&quot;font: 9px/normal &amp;quot;Times New Roman&amp;quot;; font-size-adjust: none; font-stretch: normal;&quot;&gt;&amp;nbsp; &lt;/span&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;依次打开主备播控软件、字幕机播出软件、播出审片软件，并使用自己的用户名和密码登陆。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align: left; margin-left: 0px;&quot;&gt;3、&lt;span style=&quot;font: 9px/normal &amp;quot;Times New Roman&amp;quot;; font-size-adjust: none; font-stretch: normal;&quot;&gt;&amp;nbsp; &lt;/span&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;检查切换台状态，确认切换台控制模式为“主加备”，确保&lt;/span&gt;PGM&lt;span style=&quot;font-family: 宋体;&quot;&gt;和&lt;/span&gt;PST&lt;span style=&quot;font-family: 宋体;&quot;&gt;输出信号正常；并确保&lt;/span&gt;LOGO&lt;span style=&quot;font-family: 宋体;&quot;&gt;和&lt;/span&gt;CLK&lt;span style=&quot;font-family: 宋体;&quot;&gt;键始终点亮。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align: left; margin-left: 0px;&quot;&gt;4、&lt;span style=&quot;font: 9px/normal &amp;quot;Times New Roman&amp;quot;; font-size-adjust: none; font-stretch: normal;&quot;&gt;&amp;nbsp; &lt;/span&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;检查监测系统告警器，保持音量开启。&lt;/span&gt;&amp;nbsp;&lt;/p&gt;&lt;p style=&quot;text-align: left;&quot;&gt;&lt;strong&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;二、播控软件操作&lt;/span&gt;&lt;/strong&gt;&lt;/p&gt;&lt;p style=&quot;text-align: left; margin-left: 0px;&quot;&gt;5、&lt;span style=&quot;font: 9px/normal &amp;quot;Times New Roman&amp;quot;; font-size-adjust: none; font-stretch: normal;&quot;&gt;&amp;nbsp; &lt;/span&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;加载节目单，在播控主节目单窗口查询调出当天节目单，并点击“添加”。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align: left; margin-left: 0px;&quot;&gt;6、&lt;span style=&quot;font: 9px/normal &amp;quot;Times New Roman&amp;quot;; font-size-adjust: none; font-stretch: normal;&quot;&gt;&amp;nbsp; &lt;/span&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;刷新并更新所有素材，检查所有需要更新的素材，如遇到节目名称不一致的素材，请勿更新！&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align: left; margin-left: 0px;&quot;&gt;7、&lt;span style=&quot;font: 9px/normal &amp;quot;Times New Roman&amp;quot;; font-size-adjust: none; font-stretch: normal;&quot;&gt;&amp;nbsp; &lt;/span&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;检查素材，并注意查看素材检查的报错信息；如素材未上载，及时通知上载人员上载。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align: left; margin-left: 0px;&quot;&gt;&lt;strong&gt;&lt;em&gt;8、 &lt;/em&gt;&lt;/strong&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;发送节目单，并保存。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align: left; margin-left: 0px;&quot;&gt;9、&lt;span style=&quot;font: 9px/normal &amp;quot;Times New Roman&amp;quot;; font-size-adjust: none; font-stretch: normal;&quot;&gt;&amp;nbsp; &lt;/span&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;点击“播放”。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align: left;&quot;&gt;&lt;strong&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;三、字幕操作&lt;/span&gt;&lt;/strong&gt;&lt;/p&gt;&lt;p style=&quot;text-align: left;&quot;&gt;10&lt;span style=&quot;font-family: 宋体;&quot;&gt;、确认节目单是否接收到，如没有