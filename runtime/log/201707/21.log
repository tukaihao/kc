[ info ] [ PARAM ] array (
  'template_id' => '61',
  'title' => '电视播出​的安全播出应急处理预案',
  'intro' => '',
  'catid' => '21',
  'wxqr_aid' => '',
  'wxqrcode' => '',
  'file' => '',
  'cover' => '',
  'views' => '3',
