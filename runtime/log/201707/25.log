[ sql ] [ SQL ] INSERT INTO `ppsao_wenan` (`uid` , `title` , `content` , `intro` , `author` , `cover` , `catid` , `rhref` , `rhref_text` , `is_show_title` , `is_show_statis` , `is_vip` , `views` , `likes` , `is_tmp` , `updatetime` , `dateline`) VALUES (12 , '接带岗位操作细则' , '&lt;p style=&quot;text-align: center;&quot;&gt;&lt;strong&gt;&lt;span style=&quot;font-family: 宋体; font-size: 24px;&quot;&gt;接带岗位操作细则&lt;/span&gt;&lt;/strong&gt;&lt;/p&gt;&lt;p style=&quot;line-height: 125%;&quot;&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;接带岗位作为播出部的窗口岗位，须按时到岗，认真工作，待人接物文明礼貌。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;line-height: 125%;&quot;&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;一、接带工作&lt;/span&gt;&lt;/p&gt;&lt;ol class=&quot; list-paddingleft-2&quot; style=&quot;list-style-type: decimal;&quot;&gt;&lt;li&gt;&lt;p style=&quot;color: rgb(0, 0, 0); line-height: 125%; font-size: 14px; font-style: normal; font-weight: normal; margin-top: 0px; margin-bottom: 0px;&quot;&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;整理“归还带”柜中的磁带，此柜中的磁带由磁带库或各频道收回，确认无误后，签字交接。&lt;/span&gt;&lt;/p&gt;&lt;/li&gt;&lt;li&gt;&lt;p style=&quot;color: rgb(0, 0, 0); line-height: 125%; font-family:;&quot; new=&quot;&quot; times=&quot;&quot;&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;按节目单内容，只接收三天内的播出节目带。&lt;/span&gt;&lt;/p&gt;&lt;/li&gt;&lt;li&gt;&lt;p style=&quot;color: rgb(0, 0, 0); line-height: 125%; font-family:;&quot; new=&quot;&quot; times=&quot;&quot;&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;收带时仔细核对每盘磁带的卡片填写完整性（播出频道、&lt;/span&gt;TC&lt;span style=&quot;font-family: 宋体;&quot;&gt;码、时长、播出时间、起始画面）、封条是否完好、有无终审人签名，确认无误后填写“供取带登记表”，收带。若发现有异常，立刻及时联系送带人员或节目负责人，尽快纠正并酌情记录在案，带库的收带流程照约定的方式不变。&lt;/span&gt;&lt;/p&gt;&lt;/li&gt;&lt;li&gt;&lt;p style=&quot;color: rgb(0, 0, 0); line-height: 125%; font-family:;&quot; new=&quot;&quot; times=&quot;&quot;&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;新收的磁带依据播出时间交上载人员或放入该套的同一播出时段的“未上载”柜中。&lt;/span&gt;&lt;/p&gt;&lt;/li&gt;&lt;li&gt;&lt;p style=&quot;color: rgb(0, 0, 0); line-height: 125%; font-family:;&quot; new=&quot;&quot; times=&quot;&quot;&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;收到当天纸质节目&lt;/span&gt;A&lt;span style=&quot;font-family: 宋体;&quot;&gt;（和&lt;/span&gt;B&lt;span style=&quot;font-family: 宋体;&quot;&gt;）单后，整理“备播带”柜&lt;/span&gt;,&lt;span style=&quot;font-family: 宋体;&quot;&gt;将已播完的磁带移至“归还带”柜中，需要重复播的磁带放于“重复播”柜中；清理完“备播带”柜后，按&lt;/span&gt;A&lt;span style=&quot;font-family: 宋体;&quot;&gt;（和&lt;/span&gt;B&lt;span style=&quot;font-family: 宋体;&quot;&gt;）单节目顺序准备新的备播带。栏目只备&lt;/span&gt;8&lt;span style=&quot;font-family: 宋体;&quot;&gt;分钟或&lt;/span&gt;8&lt;span style=&quot;font-family: 宋体;&quot;&gt;分钟以上的备播带，常用的公用带放于“公用带”柜中。&lt;/span&gt;&lt;/p&gt;&lt;/li&gt;&lt;/ol&gt;&lt;p style=&quot;line-height: 125%; text-indent: 0px;&quot;&gt;&lt;span style=&quot;font-family: Times New Roman;&quot;&gt;&amp;nbsp;&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;line-height: 125%; text-indent: 0px;&quot;&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;二、文件传输后的检查&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;line-height: 125%;&quot;&gt;&lt;span style=&quot;font-family: Times New Roman;&quot;&gt;1&lt;/span&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;、标清系统文件素材的检查&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;line-height: 125%;&quot;&gt;&lt;span style=&quot;font-family: Times New Roman;&quot;&gt;1.1&lt;/span&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;、用本人姓名登录上载软件，选择登录频道。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;line-height: 125%;&quot;&gt;&lt;span style=&quot;font-family: Times New Roman;&quot;&gt;1.2&lt;/span&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;、选择“素材管理视图”。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;line-height: 125%;&quot;&gt;&lt;span style=&quot;font-family: Times New Roman;&quot;&gt;1.3&lt;/span&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;、在“存储区”中选“广告存储”。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;line-height: 125%;&quot;&gt;&lt;span style=&quot;font-family: Times New Roman;&quot;&gt;1.4&lt;/span&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;、选中待审素材，进行“文件审片”。（注意：不要选同步）&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;line-height: 125%;&quot;&gt;&lt;span style=&quot;font-family: Times New Roman;&quot;&gt;1.5&lt;/span&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;、经审核通过后，进行“素材回迁”至相应播出频道。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;line-height: 125%;&quot;&gt;&lt;span style=&quot;font-family: Times New Roman;&quot;&gt;2&lt;/span&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;、高清系统文件素材的检查&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;line-height: 125%;&quot;&gt;&lt;span style=&quot;font-family: Times New Roman;&quot;&gt;2.1&lt;/span&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;、用本人姓名登录编单软件。接到节目部门的文件推送通知后，应及时记录信息并关注&lt;/span&gt;&lt;span style=&quot;font-family: Times New Roman;&quot;&gt;GMP&lt;/span&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;入库情况。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;line-height: 125%;&quot;&gt;&lt;span style=&quot;font-family: Times New Roman;&quot;&gt;2.2&lt;/span&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;、选择“素材管理视图”，在“存储区”中选“整备存储体”。选中待审素材，进行“文件审片”。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;line-height: 125%;&quot;&gt;&lt;span style=&quot;font-family: Times New Roman;&quot;&gt;2.3&lt;/span&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;、审片通过后，立即进行优先“素材回迁”。并通知当班技术人员关注迁移进度。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;line-height: 125%;&quot;&gt;&lt;span style=&quot;font-family: Times New Roman;&quot;&gt;2.4&lt;/span&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;、在播出前&lt;/span&gt;&lt;span style=&quot;font-family: Times New Roman;&quot;&gt;20&lt;/span&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;分钟，如果自动技审通过，回迁完成，则通知播出人员采用文件播出。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;line-height: 125%;&quot;&gt;&lt;span style=&quot;font-family: Times New Roman;&quot;&gt;2.5&lt;/span&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;、在播出前&lt;/span&gt;&lt;span style=&quot;font-family: Times New Roman;&quot;&gt;20&lt;/span&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;分钟，如果自动技审没有通过，或者回迁任务没有完成，则通知上载人员采用&lt;/span&gt;&lt;span style=&quot;font-family: Times New Roman;&quot;&gt;P2&lt;/span&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;卡应急上载，重新取名并通知播出人员用应急上菜的素材播出。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;line-height: 125%;&quot;&gt;&lt;span style=&quot;font-family: Times New Roman;&quot;&gt;&amp;nbsp;&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;line-height: 125%;&quot;&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;三、公共移动字幕录入和对外联络工作，有问题由技术值班人员协调。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;line-height: 125%;&quot;&gt;&lt;span style=&quot;font-family: Times New Roman;&quot;&gt;&amp;nbsp;&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;line-height: 125%;&quot;&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;四、其他事项&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;line-height: 125%;&quot;&gt;&lt;span style=&quot;font-family: Times New Roman;&quot;&gt;1&lt;/span&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;、认真填写值班日记。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;line-height: 125%;&quot;&gt;&lt;span style=&quot;font-family: Times New Roman;&quot;&gt;2&lt;/span&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;、离岗时有需要交接的事宜与上载人员口头交接清楚。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;line-height: 125%;&quot;&gt;&lt;span style=&quot;font-family: Times New Roman;&quot;&gt;3&lt;/span&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;、与上载岗位成一体，保持机房整洁，物品摆放有序，注意机房安全。&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;font-family: Times New Roman;&quot;&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; &lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align: right;&quot;&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;电视技术中心播出部&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align: right;&quot;&gt;&lt;span style=&quot;font-family: Times New Roman;&quot;&gt;2016&lt;/span&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;年&lt;/span&gt;&lt;span style=&quot;font-family: Times New Roman;&quot;&gt;3&lt;/span&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;月&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;/p&gt;' , '' , '' , '' , 21 , '' , '阅读原文' , 1 , 1 , 1 , 0 , 0 , 0 , 1500971675 , 1500971640)                   [ RunTime:0.036798s ]
